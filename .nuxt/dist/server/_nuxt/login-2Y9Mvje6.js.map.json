uccess) {\r\n    // Redirect to dashboard or home page\r\n    router.push('/dashboard');\r\n  }\r\n}\r\n\r\n// Redirect if already authenticated\r\nonMounted(() => {\r\n  if (process.client && isAuthenticated.value) {\r\n    router.push('/dashboard');\r\n  }\r\n});\r\n</script>\r\n"],"version":3}